<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.car_dealership_client.seller_controllers.SellerMainController">
    <children>
        <GridPane alignment="CENTER" layoutX="191.0" layoutY="119.0" prefHeight="176.0" prefWidth="787.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#quit" prefHeight="80.0" prefWidth="200.0" text="Выйти" textFill="RED" GridPane.columnIndex="0">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Button>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#switchToJobs" prefHeight="80.0" prefWidth="200.0" text="Jobs" GridPane.columnIndex="1">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Button>
            </children>
        </GridPane>
        <Label fx:id="workerNameHeader" alignment="CENTER" layoutX="-1.0" prefHeight="72.0" prefWidth="1000.0" text="Seller">
            <font>
                <Font name="System Bold" size="40.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
