            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#quit" prefHeight="80.0" prefWidth="200.0" text="Quit" textFill="RED" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Label fx:id="adminNameHeader" alignment="CENTER" layoutX="127.0" layoutY="14.0" prefHeight="58.0" prefWidth="343.0" text="Admin">
